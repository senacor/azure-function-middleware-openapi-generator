  "main": "index/dist.js",
  "types": "dist/index.d.ts",
  "bin": "./dist/index.js",
  "files": [
    "/dist",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/senacor/azure-function-middleware-openapi-generator/issues"
